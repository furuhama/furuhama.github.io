---
layout: null
---
{
  {% for post in site.posts %}"{{ post.url | slugify }}": {
    "title": "{{ post.title | xml_escape }}",
    "url": "{{ post.url | xml_escape }}",
    "minipost_anker": "#minipost-{{ post.date | date: "%Y-%m-%d-%H-%M-%S" }}",
    "tags": [
      {% for tag in post.tags %}"{{ tag }}"{% unless forloop.last %},
      {% endunless %}{% endfor %}
    ]
  }{% unless forloop.last %},
  {% endunless %}{% endfor %}
}
